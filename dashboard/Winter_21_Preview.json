    "insights": [
        {
            "insight": "c_Searches_by_Vertical_Returned",
            "caption": "Vertical Returned allows you to dimension Answers metrics by vertical key. Searches by Vertical Returned, for example, shows the number of searches in which each vertical was returned.",
            "x": 0,
            "y": 0,
            "width": 2,
            "height": 1
        },
        {
            "insight": "c_Clicks_by_Vertical_Returned",
            "caption": "Clicks by Vertical Returned shows the number of clicks on results in each vertical.",
            "x": 2,
            "y": 0,
            "width": 2,
            "height": 1
        },
        {
            "insight": "c_CTR_by_Vertical_Returned",
            "caption": "Finally, CTR by Vertical Returned shows the Click Through Rate of each vertical (searches with a click on each vertical divided by total searches returning that vertical).",
