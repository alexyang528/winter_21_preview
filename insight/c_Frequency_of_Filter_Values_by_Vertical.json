{
    "$id": "c_Frequency_of_Filter_Values_by_Vertical",
    "$schema": "https://schema.yext.com/config/analytics/insight/v1",
    "name": "Frequency of Filter Values by Vertical",
    "query": {
        "metrics": [
            "yext/ANSWERS_SEARCHES"
        ],
        "dimensions": [
            "yext/ANSWERS_VERTICAL_RETURNED",
            "yext/ANSWERS_FILTER_VALUE"
        ],
        "filters": [
            {
                "filter": "yext/ANSWERS_FILTER_SOURCE",
                "in": [
                    "USER_INTERFACE"
                ]
            }
        ]
    },
    "display": {
        "bar": {
            "style": "STACKED"
        }
    }
}