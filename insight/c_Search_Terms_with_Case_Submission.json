{
    "$id": "c_Search_Terms_with_Case_Submission",
    "$schema": "https://schema.yext.com/config/analytics/insight/v1",
    "name": "Search Terms with Case Submission",
    "query": {
        "metrics": [
            "yext/ANSWERS_SEARCHES",
            "yext/ANSWERS_CASE_SUBMITS"
        ],
        "dimensions": [
            "yext/ANSWERS_SEARCH_TERM"
        ],
        "filters": [
            {
                "filter": "yext/ANSWERS_HAS_CASE_SUBMIT",
                "in": [
                    "true"
                ]
            }
        ]
    },
    "display": {
        "table": {
            "sort": {
                "column": {
                    "metric": "yext/ANSWERS_SEARCHES"
                },
                "descending": true
            }
        }
    }
}