{
    "$id": "c_Voice_Searches_over_Time",
    "$schema": "https://schema.yext.com/config/analytics/insight/v1",
    "name": "Voice Searches over Time",
    "query": {
        "metrics": [
            "yext/ANSWERS_SEARCHES"
        ],
        "dimensions": [
            "yext/WEEKS"
        ],
        "filters": [
            {
                "filter": "yext/ANSWERS_HAS_VOICE_SEARCH",
                "in": [
                    "true"
                ]
            },
            {
                "filter": "yext/date",
                "in": "WEEKS_12"
            }
        ]
    },
    "display": {
        "line": {
            "showLabels": true
        }
    }
}