{
    "$id": "c_Core_Answers_Stats_by_User_Region",
    "$schema": "https://schema.yext.com/config/analytics/insight/v1",
    "name": "Core Answers Stats by User Region",
    "query": {
        "metrics": [
            "yext/ANSWERS_SESSIONS",
            "yext/ANSWERS_SEARCHES",
            "yext/ANSWERS_CLICKS",
            "yext/ANSWERS_CLICK_THROUGH_RATE"
        ],
        "dimensions": [
            "yext/ANSWERS_USER_REGION"
        ]
    },
    "display": {
        "table": {
            "sort": {
                "column": {
                    "metric": "yext/ANSWERS_SESSIONS"
                },
                "descending": true
            }
        }
    }
}