{
    "$id": "c_Searches_with_Featured_Snippets",
    "$schema": "https://schema.yext.com/config/analytics/insight/v1",
    "name": "Searches with Featured_Snippets",
    "query": {
        "metrics": [
            "yext/ANSWERS_SEARCHES"
        ],
        "dimensions": [
            "yext/WEEKS"
        ],
        "filters": [
            {
                "filter": "yext/ANSWERS_DIRECT_ANSWER_TYPE",
                "in": ["FEATURED_SNIPPET"]
            }
        ]
    },
    "display": {
        "line": {
            "showLabels": true
        }
    }
}