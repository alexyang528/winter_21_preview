{
    "$id": "c_Case_Deflections_over_Time",
    "$schema": "https://schema.yext.com/config/analytics/insight/v1",
    "name": "Case Submissions and Deflections over Time",
    "query": {
        "metrics": [
            "yext/ANSWERS_CASE_SUBMITS",
            "yext/ANSWERS_CASE_DEFLECTIONS"
        ],
        "dimensions": [
            "yext/WEEKS"
        ],
        "filters": [
            {
                "filter": "yext/date",
                "in": "WEEKS_12"
            }
        ]
    },
    "display": {
        "line": {
            "showLabels": true
        }
    }
}