{
    "$id": "c_Most_Common_Voice_Search_Terms",
    "$schema": "https://schema.yext.com/config/analytics/insight/v1",
    "name": "Most Common Voice Search Terms",
    "query": {
        "metrics": [
            "yext/ANSWERS_SEARCHES"
        ],
        "dimensions": [
            "yext/ANSWERS_SEARCH_TERM"
        ],
        "filters": [
            {
                "filter": "yext/ANSWERS_HAS_VOICE_SEARCH",
                "in": [
                    "true"
                ]
            }
        ]
    },
    "display": {
        "table": {
            "sort": {
                "column": {
                    "metric": "yext/ANSWERS_SEARCHES"
                },
                "descending": true
            }
        }
    }
}